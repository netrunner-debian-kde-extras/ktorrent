msgid "Existing files: <b>None</b>"
msgstr "Befintliga filer: <b>Inga</b>"

#: ktorrent/dialogs/fileselectdlg.cpp:534
msgid "Existing files: <b>All</b>"
msgstr "Befintliga filer: <b>Alla</b>"

