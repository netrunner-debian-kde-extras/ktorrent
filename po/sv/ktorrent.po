msgstr "Befintliga filer: <b>%1</b> av <b>%2</b>"

#: ktorrent/dialogs/fileselectdlg.cpp:542
msgid "Existing file: <b>No</b>"
msgstr "Befintlig fil: <b>Nej</b>"

